{
  "name": "graphql-cypher",
  "description": "A simple but powerful translation layer between GraphQL and Cypher",
  "keywords": [
    "graphql",
    "cypher",
    "neo4j",
    "javascript",
    "typescript"
  ],
  "version": "0.5.0",
  "author": {
    "name": "a-type (Grant Forrest)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-type/graphql-cypher"
  },
  "main": "dist/index.js",
  "umd:main": "dist/graphql-cypher.umd.production.js",
  "module": "dist/graphql-cypher.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest --watch",
    "test:integration": "jest -c=./jest.integration.config.js --forceExit --runInBand"
  },
  "peerDependencies": {
    "graphql": "^14.3.0",
    "neo4j-driver": "^1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/dockerode": "^2.5.16",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.13",
    "@types/uuid": "^3.4.4",
    "dockerode": "^2.5.8",
    "faker": "^4.1.0",
    "fs-extra": "^8.0.1",
    "graphql": "^14.3.0",
    "graphql-middleware": "^3.0.2",
    "graphql-tools": "^4.0.4",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "neo4j-driver": "^1.7.4",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.10.0",
    "ts-jest": "^24.0.2",
    "tsdx": "^0.5.11",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "ramda": "^0.26.1",
    "shortid": "^2.2.14",
    "uuid": "^3.3.2"
  }
}
